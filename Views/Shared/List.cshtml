@using VDS.RDF.Query;
@using ObjectIdentificationWebApp.Ontology;
@model List<SparqlResult>

<h2>@ViewBag.Title @Model.Count</h2>

<table style="width:100%;">
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
        @for (int d = 0; d < 3; d++)
        {            
            string prefix = ViewBag.Prefix;            
            <td><a href="@Model[i]["subject"].ToString()">@KnowledgeBase.GetNodeString(Model[i]["label"])</a></td>
           
            //Increment counter
            {
                //Increment
                i++;
                //Check for early end condition
                if (i >= Model.Count) { break; }
            }
        }
        </tr>
        --i;
    }
</table>